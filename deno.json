{
  "name": "@freshpress/fresh-plugin-sitemap",
  "version": "0.0.1",
  "description": "This plugin generates a sitemap for a Deno Fresh v2 project.",
  "runtimes": ["deno", "browser"],
  "exports": {
    ".": "./src/mod.ts"
  },
  "tasks": {
    "dev": "deno test --watch=src/ mod.ts"
  },
  "license": "Apache-2.0",
  "imports": {
    "@std/path": "jsr:@std/path@^1",
    "@std/fs": "jsr:@std/fs@^1"
  },
  "fmt": {
    "options": {
      "semiColons": false,
      "singleQuote": true
    }
  }
}
